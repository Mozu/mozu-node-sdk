{
  "name": "mozu-node-sdk",
  "description": "Mozu JavaScript SDK for Node.js and Arc.js environments",
  "author": "Volusion, LLC",
  "scripts": {
    "lint": "eslint src",
    "prebabel": "npm run lint",
    "babel": "babel src -d .",
    "preci-build": "npm run babel",
    "ci-build": "npm test",
    "ci-build-stable": "npm run ci-build",
    "postci-build-stable": "mozu-validate-npm-package",
    "cov": "istanbul cover test/*.js",
    "test": "tape ./test/*.js",
    "spec": "npm run test | tap-spec",
    "preversion": "npm run spec",
    "version": "git add -A ."
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "version": "2.13.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/Mozu/mozu-node-sdk"
  },
  "dependencies": {
    "concat-stream": "^1.5.1",
    "mozu-metadata": "^2.3.6",
    "tunnel-agent": "^0.4.2",
    "uri-template": "^1.0.0"
  },
  "devDependencies": {
    "acorn": "^2.7.0",
    "babel-cli": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "body-parser": "^1.14.2",
    "eslint": "^1.10.3",
    "glob": "^6.0.4",
    "istanbul": "^0.4.2",
    "jort": "^1.3.0",
    "mozu-multipass": "^0.3.1",
    "mozu-validate-npm-package": "^1.2.1",
    "sinon": "^1.17.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.4.0"
  },
  "license": "MIT",
  "engines": {
    "node": ">=4.1.0"
  }
}
